{
	"$schema": "https://schema.tauri.app/config/2.0.0-rc",
	"productName": "sous-chef",
	"version": "0.1.0",
	"identifier": "com.tauri.dev",
	"build": {
		"frontendDist": "..",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "npm start",
		"beforeBuildCommand": "npm run build"
	},
	"app": {
		"windows": [
			{
				"title": "SousChef",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
